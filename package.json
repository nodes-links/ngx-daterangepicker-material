{
  "name": "ngx-daterangepicker-material",
  "version": "3.0.4",
  "description": "Angular 2+ date range picker (with material design theme)",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "ng2",
    "daterangepicker",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "moment",
    "picker"
  ],
  "homepage": "https://github.com/fetrarij/ngx-daterangepicker-material",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve",
    "deploy": "ng deploy",
    "demo:build": "ng build --prod",
    "test": "./node_modules/.bin/ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --prod --base-href=\"/ngx-daterangepicker-material/\" && ngh --dir demo/dist",
    "build": "ng-packagr -p ng-package.json && npm run build-themes && npm run bundle-styles",
    "build-themes": "bash ./tools/build-themes.sh",
    "bundle-styles": "scss-bundle -c scss-bundle.config.json",
    "build:schematics": "tsc -p ./tsconfig.schematics.json",
    "copy:schematics": "cpr schematics/dist dist/schematics && cp ./schematics/collection.json ./dist/schematics && cp ./schematics/ng-add/schema.json ./dist/schematics/ng-add",
    "postbuild": "npm run build:schematics && npm run copy:schematics",
    "prettier": "prettier --write '**/*.{html,js,ts,css,scss,less,md,json}'",
    "prettier-check": "prettier --list-different '**/*.{html,js,ts,css,scss,less,md,json}'"
  },
  "author": "Fetrarijaona R.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fetrarij/ngx-daterangepicker-material.git"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.7",
    "@angular/animations": "15.2.8",
    "@angular/cdk": "15.2.8",
    "@angular/cli": "15.2.0",
    "@angular/common": "15.2.8",
    "@angular/compiler": "15.2.8",
    "@angular/compiler-cli": "15.2.8",
    "@angular/core": "15.2.8",
    "@angular/forms": "15.2.8",
    "@angular/material": "15.2.8",
    "@angular/platform-browser": "15.2.8",
    "@angular/platform-browser-dynamic": "15.2.8",
    "@angular/router": "15.2.8",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.34",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^6.0.2",
    "core-js": "3.30.2",
    "cpr": "^3.0.1",
    "jasmine-core": "5.0.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "moment": "^2.24.0",
    "ng-packagr": "15.2.2",
    "ngx-highlightjs": "^4.1.0-beta",
    "node-sass": "8.0.0",
    "prettier": "^2.0.2",
    "protractor": "7.0.0",
    "rxjs": "^6.5.5",
    "scss-bundle": "^3.1.1",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "tslint": "~5.11.0",
    "typescript": "4.9.5",
    "zone.js": "^0.13.0"
  }
}
